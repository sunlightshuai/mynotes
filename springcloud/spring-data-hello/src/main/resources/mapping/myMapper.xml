<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mydata.mapper.SubMapper">

    <select id="select" resultType="HashMap">
        select * from tbl_emp where emp_id = #{id}
    </select>

    <select id="selectAll" resultType="HashMap">
        select * from tbl_emp
    </select>

    <insert id="insert" parameterType="HashMap" >
        insert into tbl_emp (emp_id,emp_name,gender,email,d_id)
        values (#{emp_id,jdbcType=INTEGER},#{emp_name,jdbcType=VARCHAR},#{gender,jdbcType=VARCHAR},#{email,jdbcType=VARCHAR},#{d_id,jdbcType=INTEGER})
    </insert>

